    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-cache": "^5.1.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "xlsx": "^0.18.5"
  },
