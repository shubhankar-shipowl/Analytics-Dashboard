{
  "name": "dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for Analytics Dashboard",
  "main": "server.js",
  "scripts": {
    "start": "node --max-old-space-size=4096 server.js",
    "dev": "nodemon --exec \"node --max-old-space-size=4096\" server.js",
    "start:verbose": "set NODE_ENV=development&& node --max-old-space-size=4096 server.js",
